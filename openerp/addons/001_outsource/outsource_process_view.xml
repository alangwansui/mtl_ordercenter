			           <button name="approve_line"  states="w_receive,done" string="approve line" type="object" icon="icon" groups="base.group_no_one"/>
			        </group> 
	        </group>
		     <notebook colspan='4'>
		         <page string='outsource process info'>
			         
			      		<field name='outsource_delivery_ids'  attrs="{'readonly':[('state','=','done')]}">
								<tree string='process lines'>
								
			      			        <field name='production_id'/>
			      			        <field name='process_types'/>
			      			        <field name='board_types'/>
			      			        <field name='receive_board'/>
						            <field name="product_id"/>
						            <field name="product_qty" />
									<field name='qty_pnl'/>
						            <field name='qty_receive' />
									<field name='qty_pnl_receive' editable='bottom'/>
								    <field name='receive_area'/>
						            <field name='product_unit'/>
									<field name="pcs_price_units" />
							       	<field name='pnl_price_units'/>
							       	<field name='cost_total' sum='cost total'/>
							       	<field name='board_thickness'/>
							       	<field name='hole_count'/>
							       	<field name='board_type'/>
									<field name='length'/>
									<field name='width'/>
									<field name='pnl_length'/>
									<field name='pnl_width'/>
									<field name="process_args" />
									<field name='delivery_date'/>
									<field name='finish_time'/>
								    <field name='state'/>
									 <field name='note'/>
			      			    </tree>
			      			    <form string='process lines'>
			      			        <group colspan='4' col='6'>
							               <group colspan='1' col='1'>
							           
								    			<field name='production_id' attrs="{'readonly':[('process_lines_id','!=',False)]}"/>
								    			
								    			<field name='process_lines_id' on_change='onchange_process_lines_id(process_lines_id)'/>
								    			<field name='outsource_number'/>
								    			
								    			<field name='process_type'/>
								    			<field name='process_types' readonly="1"/>
								    			<field name="product_id"/>
							                   	<field name='outsource_apply_id' invisible='1'/>
											 
									           	 <field name='delivery_date'/>
									           	  <field name='dispatch_state'  attrs="{'invisible':[('state','!=','w_receive')]}"/>
									           		<field name='delivery_time'/> 
									           		<field name='eng_file' filename='eng_filename'/>
									     
									         		
									       </group>
									       <group colspan='1' col='1'>
									         
				          						<field name='current_workcenter_id'/>
												<field name='length'/>
									           	<field name='width'/>
									           	<field name='product_unit'/>
									           	<field name='pnl_length'/>
									           	<field name='pnl_width'/>
									           	<field name='pnl_unit'/>
									           	 <field name='product_area'/>
									           	 <field name='receive_area'/>
									           	 <field name='eng_filename' readonly='1'/>
									        </group>
									        <group colspan='1' col='1'>
									          
									           <field name='layer_count'/>
									          <field name='board_thickness'/>
									           <field name="product_qty" />
									           <field name='qty_pnl'/>
									           <field name='pcs_unit_count'/>
									           <field name='qty_receive'  attrs="{'invisible':[('state','=','draft')]}"/>
									           <field name='scrap_qty'  attrs="{'invisible':[('state','=','draft')]}"/>
											   <field name='qty_pnl_receive' attrs="{'invisible':[('state','=','draft')]}"/>
			           					       <field name='scrap_pnl_qty' attrs="{'invisible':[('state','=','draft')]}"/>
												
												<field name='delivery_id'/>
												<field name='partner_id' invisible='1'/>
												<field name='return_numbers'/>
									         </group>
									         <newline/>
									         <group colspan='2' col='4'>
									             <field name='process_args' colspan='4'/>
									             <newline/>
									             <field name='process_args_info' colspan='4'/>
									         </group>
								        </group>
								     
								        <group colspan='2' col='2'>
								            <group colspan='1' col='1'>
								                <field name='top_area'/>
		  										<field name='bottom_area'/>
		  										<field name='top_bot_area'/>
		  										<field name="gold_thickness" />
		  										<field name="nickel_thickness"/>
		  										<field name='sn_thickness'/>
								            </group>
								            <group colspan='1' col='1'>
								                <field name='gold_fingers'/>
								                
								           	     <field name='red_tape_article'/>
								           	     <field name='silver_thickness'/>
								   
								                 <field name="pcs_price_units" />
									             <field name='default_min_cost' readonly='1'/>
									             <field name='meter'/> 	 
								            </group>
								            <group colspan='2' col='2'>
								                <group colspan='1' col='1'>
									             	<field name='board_types'/>
			  										<field name='if_have_tax'/>
			  										<field name='account_tax_id'/>
			  										<field name='pnl_price_units'/>
									           	 	<field name='cost_total' sum='cost total'/>
		  										</group>
		  										<newline/>
		  										<button name='production_read' string='production read' icon='gtk-jump-to' type='object' invisible='1'/>
								            </group>
								        </group>
								        <group colspan='2' col='2'>
								           	<notebook colspan='2'>
								         
								           	    <page string='plasma_gong_side_VCUT info'>
								           	        <field name='board_type'/>
								           	        <newline/>
								           	        <group colspan='2' col='2'>
								            			<field name="cutter_size" />
										         		<field name='vcut_size'/>
										         		<field name='v_cutter_count'/>
										         		<field name='note'/>
										         	</group>
										         	<group colspan='2' col='2'>
										         	    <field name='gong_size'/>
										         	  
										         		<field name='gong_slot_count'/>
										         		<field name='sink_hole_count'/>
										         		<field name='horn_hole_count'/>
										         		<field name='plate_hole_count'/>
										         		<field name='pd_thick'/>
										         	</group>
								           	    </page>
								           	    <page string='fly_test info'>
								           	        <group colspan='2' col='2'>
													    <field name="points_count" />
										            	<field name='total_count'/>
												         <field name='if_first_test'/>
												         <field name='aoi_test_count'/>
													</group>
								           	        <group colspan='2' col='2'>
								           	            <field name='if_retest'/>
												         <field name='if_fpc_sample' />
												         <field name='if_low_resistance'/>
												         <field name='pcb_remove'/>
								           	        </group>
								           	    </page>
								           	    <page string='drill info'>
								           	        <group colspan='2' col='2'>
												         <field name="drills_file"  filename='drills_filename'/>
										            	<field name="min_hole_dia" />
										            	<field name='board_material'/>
												  
												         <field name='hole_count'/>
												         <field name='cu_thickness'/>
								            		</group>
								            		<group colspan='2' col='2'>
								            		    <field name='drills_filename' readonly='1'/>
								            		    <field name="slot_hole_count" />
												         <field name='join_hole_count'/>
												         <field name="special_hole_count" />
												         <field name='plate_hole_count'/>
												      
								            		</group>
								           	    </page>
								           	    <page string='gold finger info'>
								           	        <group colspan='1' col='1'>
								           	            <field name='gold_finger_width'/>
								           	            <field name='gold_finger_width2'/>
								           	            <field name='gold_finger_width3'/>
								           	            <field name='gold_finger_width4'/>
								           	            <field name='gold_finger_width5'/>
								           	            <field name='gold_finger_width6'/>
								           	        </group>
								           	        <group colspan='1' col='1'>
								           	            <field name='gold_finger_height'/>
								           	            <field name='gold_finger_height2'/>
								           	            <field name='gold_finger_height3'/>
								           	            <field name='gold_finger_height4'/>
								           	            <field name='gold_finger_height5'/>
								           	            <field name='gold_finger_height6'/>
								           	        </group>
								           	        <group colspan='1' col='1'>
								           	            <field name='finger_count'/>
								           	            <field name='finger_count2'/>
								           	            <field name='finger_count3'/>
								           	            <field name='finger_count4'/>
								           	            <field name='finger_count5'/>
								           	            <field name='finger_count6'/>
								           	        </group>
								           	    </page>
								           	  	<page string='wire_drawing_OSP info'>
								           	  	    <group colspan='1'  col='1'>
								           	  	        <field name='wire_draw_types'/>
								           	  	        <field name='norms_mark'/>
								           	        	<field name='film_thickness'/>
								           	  	    </group>
								           	  	</page>
								           	  	<page string='sink_sliver info'>
								           	  	    <group colspan='1'  col='1'>
								           	  	        <field name='sink_sliver_type'/>
								           	      		<field name='cut_type'/>
								           	      		<field name='laminate_types'/>
								           	      		<field name='laminate_board_thickness'/>
								           	      	</group>	
								           	      	<group colspan='1'  col='1'>	
								           	      		<field name='repair_type'/>
								           	      		<field name='osp_type'/>
								           	      		<field name='length_pp'/>
								           	      		<field name='laminate_cu_thickness'/>
								           	  	    </group>
								           	  	</page>
								          
								           	  	
								           	  	
								           	  	
								           	</notebook>
								      	</group>
								      <field name='state'/>
								       <button name='process_cost_compute' string='process cost compute' icon='' type='object' invisible='1'/>
			      			    </form>
			      			 </field>
			      			 <button name='production_read' string='production read' icon='gtk-jump-to' type='object' attrs="{'invisible':[('state','=','done')]}" groups="base.group_no_one"/>
			      			 <button name='process_cost_compute' string='process cost compute' icon='' type='object' attrs="{'invisible':[('type','=','plasma_process')],'readonly':[('state','=','done')]}" groups="base.group_no_one"/>
			      			 <button name='plasma_amount' string='plasma amount' icon='' type='object' attrs="{'invisible':[('type','!=','plasma_process')],'readonly':[('state','=','done')]}" groups="base.group_no_one"/>
			      			 
		      	  
		      		<group colspan='1'>
			           	<field name='reason_apply'/>
			              <newline/>
			            <field name="process_content"/>
			             <newline/>
				        <field name="process_require" />
		      		</group>
		      	</page> 
				</notebook>
		         <newline/> 
            	<!--  <field name="state" readonly="True"/>  -->
    	   	
    		</sheet>
       	</form>
   </field>
